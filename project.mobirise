          "_customHTML": "<section data-bs-version=\"5.1\" class=\"mbr-section\" plugins=\"audiobar\" group=\"Zen2cool Blocks\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n      <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\n\t <header>Player ID</header>\n\t\t        <input type=\"range\" inline title=\"Player ID (must be unique in each audio block on page)\" name=\"NumberPlayer\" min=\"1\" max=\"100\" step=\"1\" value=\"1\">v\n\t\t\n\t\t <header>Basic Settings</header>\n\t\t\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"range\" name=\"textWidth\" title=\"Width\" min=\"4\" max=\"12\" value=\"5\" step=\"1\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n                <input type=\"checkbox\" title=\"Texte\" name=\"showSubtxt\" checked>\n\n <header>Player Config</header>\n        <input type=\"range\" inline title=\"Player Width\" name=\"PlaySize\" min=\"300\" max=\"1000\" step=\"1\" value=\"493\">\n                <input type=\"range\" inline title=\"Player Radius\" name=\"PlayRad\" min=\"0\" max=\"100\" step=\"1\" value=\"38\">\n                       <input type=\"range\" inline title=\"Player Size Breakpoint\" name=\"Breakpoint\" min=\"500\" max=\"1500\" step=\"25\" value=\"900\">\n        <input type=\"color\" title=\"Player Color\" name=\"PlayerCol\" value=\"#0021a6\">\n        <select title=\"Position\" name=\"Positionpl\">\n        <option value=\"star\">Left</option>\n        <option value=\"center\" selected>Center</option>\n         <option value=\"end\">Right</option>\n        </select>\n\n       <header>Buttons Color</header>\n        <input type=\"color\" title=\"Play/Pause Button\" name=\"PlPaCol\" value=\"#ffffff\">\n                   <input type=\"checkbox\" title=\"Volume On/Off\" name=\"Volumeoff\" checked>\n         <input type=\"color\" title=\"Volume Button\" name=\"VolCol\" value=\"#ffffff\" condition=\"Volumeoff\">\n         <input type=\"color\" title=\"Volume Push Color\" name=\"VolumeCol\" value=\"#000000\" condition=\"Volumeoff\">  \n\t\t <input type=\"color\" title=\"Background Volume\" name=\"VolColBack\" value=\"#005d2f\" condition=\"Volumeoff\">\n        <input type=\"range\" inline title=\"Vol Background Opacity\" name=\"VolBckCol\" min=\"0\" max=\"1\" step=\"0.1\" value=\"1\" condition=\"Volumeoff\">\n        <input type=\"color\" title=\"Slider Color\" name=\"SliderCol\" value=\"#ffffff\">\n        <input type=\"color\" title=\"Progress Color\" name=\"PinCol2\" value=\"#000000\">\n           <input type=\"checkbox\" title=\"Dot On/Off\" name=\"Dotonoff\" checked>\n        <input type=\"color\" title=\"Dot Progress Color\" name=\"ProgressCol\" value=\"#8c0404\" condition=\"Dotonoff\">\n        <input type=\"color\" title=\"Time Color\" name=\"TimeCol\" value=\"#ffffff\">\n                <header>Download Button</header>  \n        <select title=\"Download Button\" name=\"Downbutton\">\n        <option value=\"player-with-download\" selected>On</option>\n        <option value=\"player\">Off</option>\n        </select>\n         <input type=\"color\" title=\"Download Color\" name=\"DownCol\" value=\"#f0f0f5\" condition=\"Downbutton == 'player-with-download'\">\n       \n        \n<header>Audio URL</header>\n            <input type=\"text\" title=\"Audio URL\" name=\"audioSrc\" value=\"https://jesusoldtestamentprophecies165008-staging.s3.us-east-2.amazonaws.com/public/Jesus_Every_Word_and_Deed_Audio_Excerpt.mp3\">\n            <input type=\"checkbox\" title=\"Loop\" name=\"loopoption\">\n            <input type=\"checkbox\" title=\"Autoplay\" name=\"Autoplay\">\n                    <select title=\"Preload\" name=\"PreLoad\">\n        <option value=\"auto\">On</option>\n        <option value=\"metadata\" selected>Metadata</option>\n               </select>\n                \n                        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/lionaf.jpg\" parallax>\n            <input type=\"color\" title=\"Background Color\" value=\"#f4f4f4\" selected>\n\n        </fieldset>\n                \n         <input type=\"color\" title=\"Color 2\" name=\"color2\" value=\"#f4f4f4\" condition=\"bg.type == 'color'\">\n         <input type=\"range\" name=\"angle\" title=\"Angle\" min=\"0\" max=\"270\" value=\"0\" step=\"1\" condition=\"bg.type == 'color'\">\n\t\t <input type=\"checkbox\" title=\"Background Transparent\" name=\"transp\" condition=\"bg.type == 'color'\">\n\t\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232323\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0\" condition=\"overlay && bg.type !== 'color'\">\n        \n           \n    <!-- End block parameters -->\n    </mbr-parameters>\n       <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\">\n    </div>\n    \n    <div class=\"row justify-content-center\">\n            <div class=\"col-md-12 col-lg-{{textWidth}}\">\n                <h3 class=\"mbr-section-title mbr-fonts-style mb-4\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-2\"><b>Audio Excerpt</b></h3>\n                <h4 class=\"mbr-section-subtitle mbr-fonts-style mb-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitl